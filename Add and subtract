using System;

namespace AddAndSubtract
{
    class Program
    {
        static void Main(string[] args)
        {
            int n1 = int.Parse(Console.ReadLine());
            int n2 = int.Parse(Console.ReadLine());
            int n3 = int.Parse(Console.ReadLine());
            int d = Sum(n1,n2) - n3;
            Console.WriteLine(d);
        }
        static int Sum(int n1, int n2)
        {
            int s = n1 + n2;
            return s;
        }
         
    }
}
